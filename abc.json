{
  "name": "HelloWorld",
  "options": {
    "type": "uxcore",
    "template": "https://github.com/nowa-webpack/template-uxcore/archive/v2.zip",
    "npm": "npm",
    "vars": {
      "__LOCAL__": true
    },
    "buildvars": {
      "__LOCAL__": [false]
    },
    "pages": false,
    "externals": {
      "natty-fetch": "var nattyFetch",
      "react": "var React",
      "react-dom": "var ReactDOM",
      "react-router": "var ReactRouter",
      "Promise": "var Promise",
      "reflux": "var Reflux",
      "uxcore": "var Uxcore",
      "lodash": "var _"
    },
    "mockapi": {
      "/mock/query/getSomeInfo.json": "mock/query/getSomeInfo.js"
    },
    "libraries": {
      "Uxcore": {
        "output": "uxcore.js",
        "mappings": {
          "Table": "uxcore-table@~1.12.13",
          "Menu": "uxcore-menu@~1.4.1"
        }
      }
    }
  }
}
